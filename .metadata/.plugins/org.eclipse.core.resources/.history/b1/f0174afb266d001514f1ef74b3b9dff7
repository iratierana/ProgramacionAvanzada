package JuegoElfosOrcos;

public class Criatura {

	int vida;
	String nombre;
	Artefacto artefacto;
	
	public Criatura(int vida, String nombre, Artefacto artefacto) {
		super();
		this.vida = vida;
		this.nombre = nombre;
		this.artefacto = artefacto;
	}

	@Override
	public String toString() {
		return "Criatura [vida=" + vida + ", nombre=" + nombre + ", artefacto="
				+ artefacto.toString() + "]";
	}
	
	
}
